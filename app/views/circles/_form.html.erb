<%= form_with(model: @circle, local: true) do |f| %>
  <%= f.label :circle_name %>
  <%= f.text_field :circle_name %>
  <%= f.label :circle_content %>
  <%= f.text_field :circle_content %>
  <%= f.label :circle_image %>
  <%= f.text_field :circle_image %>
  <%= f.hidden_field :circle_image_cache %>
  <%= f.submit %>
<% end %>
