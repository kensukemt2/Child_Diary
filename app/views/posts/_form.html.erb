<%= form_with(model: @post, local: true) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.hidden_field :image_cache %>
  <%= f.submit "新規作成する" %>
<% end %>
<%= link_to "マイページに戻る", user_path(current_user.id) %>
